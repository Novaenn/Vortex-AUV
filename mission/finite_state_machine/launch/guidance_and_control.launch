<?xml version="1.0"?>
<launch>

  <include file="$(find vortex)/launch/dp_control.launch"/>

  <arg name="filename" default="$(find waypoint_action_client)/waypoints/square_waypoints.yaml"/>
  <node pkg="waypoint_action_client"
        type="waypoint_tracking_client.py"
        name="load_waypoints"
        output="screen">
          <rosparam subst_value="true">
            filename: $(arg filename)
          </rosparam>
   </node>

   <node
    name="service_client"
    pkg="finite_state_machine"
    type="service_client.py"
    output="screen">
   </node> 

</launch>
